<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">

<params name="Cavallo 570 S1" type="template">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="21" val="8"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="24" val="8"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-3.0"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="3.0"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.05"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.41"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.01"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="0.83"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="0.72"/>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="25" val="23"/>
  </section>
  
  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.75" val="0.5"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="45000"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="300" val="90"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="-2.5"/>
    <attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="300" val="90"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="-2.5"/>
    <attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="-2.5"/>
    <attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="300" val="100"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
    <attnum name="camber" min="-5" max="5" unit="deg" val="-2.5"/>
    <attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
  </section>
  
  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="200" max="2500" val="1500"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="lbs/in" min="1" max="2500" val="500"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="6500"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="280"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="320"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="40"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="6500"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="280"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="320"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="40"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="5200"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="90"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="15"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="5200"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="90"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="80"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="15"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
  </section>

</params>
