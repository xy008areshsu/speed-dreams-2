<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Viper GTS-R" type="template">

	<section name="hymie">
		<attnum name="WingRInverse" min="0.0" max="1.0" val="0.0004"/>
		<attnum name="TireAccel" min="1.0" max="30.0" val="11.5"/>
		<attnum name="MaxBrake" min="1.0" max="60.0" val="16.0"/>
		<attnum name="SlipLimit" min="0.0" max="3.0" val="1.40"/>
		<attnum name="SteerSkid" min="0.0" max="1.0" val="0.08"/>
		<attnum name="SteerGain" min="0.0" max="3.0" val="1.30"/>
		<attnum name="SteerGain2" min="0.0" max="3.0" val="1.02"/>
		<attnum name="SteerLimit" min="0.0" max="3.0" val="0.70"/>
		<attnum name="TrackFriction" min="0.0" max="3.0" val="1.00"/>
		<attnum name="CamberEffect" min="0.0" max="1.0" val="0.0"/>
		<attnum name="BTBoost" min="0.0" max="10.0" val="1.0"/>
		<attnum name="AvoidBTBoost" min="0.0" max="10.0" val="1.0"/>
		<attnum name="Learning" min="0.0" max="10.0" val="0.0"/>
		<attnum name="ABSFactor" min="0.0" max="10.0" val="0.80"/>
		<attnum name="TeamWaitDistance" min="0.0" max="1000.0" val="80.0"/>
		<attnum name="MaxIncFactor" min="0.0" max="10.0" val="2.8"/>
		<attnum name="AvoidSpeed" min="-20.0" max="20.0" val="-2.0"/>
		<attnum name="Skill" val="0.6"/>
	</section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="29"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="30" val="6.8"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.50"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="42000"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.3"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
    <attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.3"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="40"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>

</params>














